{
  "name": "unocss-preset-strict-design",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "!/**/__test__",
    "!*.test.*"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint .",
    "test": "vitest run",
    "smoke": "yarn build && yarn lint && yarn test",
    "-- PRE-COMMIT HOOKS --": "",
    "localAfterInstall": "husky || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "packageManager": "yarn@4.7.0",
  "dependencies": {
    "unocss": "^66.1.0-beta.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.0",
    "pinst": "^3.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.9"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericvera/unocss-preset-strict-design"
  },
  "keywords": [
    "unocss",
    "atomic-css",
    "unocss-preset",
    "css",
    "tailwind"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,mjs}": "eslint --cache",
    "*": "prettier --ignore-unknown --write"
  }
}
